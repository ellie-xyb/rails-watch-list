<div class="container mf-3">
  <div class="row">
    <div class="col-10">
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :bg_url %>
        <%= f.submit "Add", class: "btn btn-primary" %>
      <% end %>
      <br>
      <%= link_to "Back", lists_path, class: "btn btn-outline-warning" %>
    </div>
  </div>
</div>

